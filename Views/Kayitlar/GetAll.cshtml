@model IEnumerable<KURSOTOMASYON.Models.KAYIT>

<h1>KESİN KAYITLAR</h1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KAYIT_REFNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EGITIM_GRUP.EGITIM_GRUP_ADI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KURSIYER.ADI_SOYADI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACIKLAMA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KAYIT_DURUMU)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KAYIT_UCRETI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TAKSIT_SAYISI)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KAYIT_REFNO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EGITIM_GRUP.EGITIM_GRUP_ADI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KURSIYER.ADI_SOYADI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACIKLAMA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KAYIT_DURUMU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KAYIT_UCRETI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TAKSIT_SAYISI)
            </td>
        </tr>
    }

</table>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $("#footer").hide();
        $("#sidebarCollapse").hide();
        $("#hesapyonet").hide();
    });

</script>