@model IEnumerable<KURSOTOMASYON.Models.EGITMEN_HAREKET>

<h1>EĞİTMEN HAREKETLERİ</h1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EGITMEN_HAREKET_REFNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EGITMan.ADI_SOYADI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EGITMan.TC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EGITMan.DURUM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TARIH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ALACAK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ODENEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACIKLAMA)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EGITMEN_HAREKET_REFNO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EGITMan.ADI_SOYADI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EGITMan.TC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EGITMan.DURUM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TARIH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ALACAK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ODENEN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACIKLAMA)
            </td>
        </tr>
    }

</table>


<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $("#footer").hide();
        $("#sidebarCollapse").hide();
        $("#hesapyonet").hide();
    });

</script>