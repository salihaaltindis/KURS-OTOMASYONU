@model IEnumerable<KURSOTOMASYON.Models.KATEGORI>

<h1>KATEGORİLER</h1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KATEGORI_REFNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KATEGORI_ADI)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KATEGORI_REFNO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KATEGORI_ADI)
            </td>
        </tr>
    }

</table>


<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $("#footer").hide();
        $("#sidebarCollapse").hide();
        $("#hesapyonet").hide();
    });

</script>