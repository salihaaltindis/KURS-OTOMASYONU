            <td>
                @Html.DisplayFor(modelItem => item.ADRES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EMAIL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACIKLAMA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GORUSME_TARIHI)
            </td>
        </tr>
    }

</table>
