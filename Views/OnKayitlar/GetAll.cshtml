        <th>
            @Html.DisplayNameFor(model => model.ADRES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMAIL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACIKLAMA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GORUSME_TARIHI)
        </th>
        <th></th>
