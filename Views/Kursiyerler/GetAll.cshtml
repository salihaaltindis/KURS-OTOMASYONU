<h1>KURSİYERLER</h1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KURSIYER_REFNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ADI_SOYADI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ADRES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TELEFON)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PAROLA)
